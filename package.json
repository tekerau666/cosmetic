    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "unit": "jest --config ./config/jest/jest.config.ts",
    "srbk": "storybook dev -p 6006 -c ./config/storybook",
    "srbk:build": "storybook build -c ./config/storybook",
    "loki": "npx loki test",
    "loki:ok": "npx loki approve",
    "loki:ci": "loki --requireReference --reactUri file:./storybook-static",
    "comment": "перед запуском reg-cli скриптов необходимо запустить сторибук",
    "reg-cli:json": "node scripts/generate-visual-json-report.js",
    "reg-cli:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky install"
